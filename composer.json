{
    "name": "contentful/contentful",
    "description": "SDK for the Contentful Content Delivery API",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "contentful/core": "2.0.0-beta1",
        "contentful/rich-text": "1.0.0-beta4",
        "psr/cache": "^1.0",
        "symfony/console": "~2.7|~3.0|~4.0",
        "symfony/filesystem": "~2.7|~3.0|~4.0",
        "cache/void-adapter": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "php-vcr/phpunit-testlistener-vcr": "3.0.0|^3.2",
        "cache/array-adapter": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Contentful\\Delivery\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contentful\\Tests\\Delivery\\": "tests"
        },
        "files": ["vendor/contentful/core/scripts/TestCase.php"]
    },
    "bin": ["bin/contentful"],
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.0-dev"
        }
    },
    "suggest": {
        "symfony/cache": "A PSR-6 compatible cache component"
    }
}
